# ========== OPENAI ==========
# API Key para servicios de IA (OpenAI GPT)
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# ========== SUPABASE ==========
# URL de tu proyecto Supabase
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
# Clave anónima (pública) de Supabase
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ========== STRIPE ==========
# Clave pública de Stripe para pagos
EXPO_PUBLIC_STRIPE_PUBLIC_KEY=your_stripe_public_key_here

# ========== GEMINI ==========
# API Key para Google Gemini AI
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# ========== PRIVATE KEYS (NO PUBLICAR) ==========
# Estas claves NO deben tener el prefijo EXPO_PUBLIC_ 
# y NO deben incluirse en el código del cliente
OPENAI_API_KEY=
SUPABASE_SERVICE_KEY=
STRIPE_SECRET_KEY=
