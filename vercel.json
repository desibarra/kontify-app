{
    "buildCommand": "npx expo export --platform web",
    "outputDirectory": "dist",
    "devCommand": "npx expo start --web",
    "installCommand": "npm install --legacy-peer-deps",
    "framework": null,
    "functions": {
        "api/**/*.ts": {
            "runtime": "edge"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}